{
  "name": "agapito-dot-dev",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "sync": "aws s3 sync --cache-control 'max-age=31536000' dist/ \"s3://agapito-dot-dev\" --delete",
    "deploy": "yarn build && yarn sync && yarn invalidate",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E3DD9C15MXRE55 --paths \"/*\""
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "aws-sdk": "^2.896.0",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vue-tsc": "^0.0.24"
  }
}