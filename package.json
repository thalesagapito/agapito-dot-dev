{
  "name": "agapito-dot-dev",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite-ssg build",
    "serve": "vite preview",
    "sync": "aws s3 sync --cache-control 'max-age=31536000' dist/ \"s3://agapito-dot-dev\" --delete --acl public-read",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E3DD9C15MXRE55 --paths \"/*\"",
    "deploy": "yarn build && yarn sync && yarn invalidate"
  },
  "dependencies": {
    "@vueuse/core": "^4.9.1",
    "dayjs": "^1.10.4",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.4",
    "@tailwindcss/typography": "^0.4.0",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/server-renderer": "^3.0.11",
    "@vueuse/head": "^0.5.1",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.896.0",
    "eslint": "^7.25.0",
    "postcss": "^8.2.13",
    "postcss-nested": "^5.0.5",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vite-plugin-components": "^0.8.4",
    "vite-plugin-pages": "^0.10.0",
    "vite-ssg": "^0.9.3",
    "vue-router": "^4.0.6",
    "vue-tsc": "^0.0.24"
  }
}